libs:: $(FINAL_TARGET)/chrome/langpack-uk@waterfox.manifest
	$(PYTHON) $(topsrcdir)/config/nsinstall.py \
	  $(srcdir)/langpack-uk@waterfox \
          $(foreach exclude,$(exclude_files), -X $(srcdir)/langpack-uk@waterfox/$(exclude)) \
          $(FINAL_TARGET)/chrome
	$(call py_action,buildlist,$(FINAL_TARGET)/chrome.manifest 'manifest chrome/langpack-uk@waterfox.manifest')
	
